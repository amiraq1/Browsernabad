{
    "manifest_version": 2,
    "name": "NabdhExtractor",
    "version": "1.0",
    "description": "Extracts main content for AI processing",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "permissions": [
        "<all_urls>"
    ]
}